{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "!(**/*.@(js|css|png|json)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source" : "/sw.js",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "60"
          }
        ]
      }
    ]
  }
}
